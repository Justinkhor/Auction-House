<br><br><br>
<div class = "container col-md-offset-4 col-md-4 col-md-offset-4">
<h1>Login</h1>

<%= form_tag '/login' do %><br>

  Email: <%= text_field_tag :email %><br><br>
  Password: <%= password_field_tag :password %><br><br>
  <%= submit_tag "Submit" %><br><br>
  Don't Have An Account? <%= link_to 'Sign Up Now', new_user_path %>


<% end %>
</div>
